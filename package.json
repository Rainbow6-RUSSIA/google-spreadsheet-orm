{
  "name": "google-spreadsheet-orm",
  "version": "0.0.6",
  "author": {
    "name": "Blackbox Team",
    "email": "blackbox@economia.cz",
    "url": "https://www.economia.cz/"
  },
  "license": "MIT",
  "description": "Use google sheets for small database-like projects.",
  "keywords": [
    "google",
    "sheets",
    "spreadsheets",
    "orm",
    "api",
    "database",
    "database-like",
    "admin",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/economia/google-spreadsheet-orm.git"
  },
  "scripts": {
    "build": "tsc",
    "format": "npm run prettier -- --write",
    "test": "jest",
    "prettier": "prettier **/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "postinstall": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^24.0.9",
    "@types/node": "^12.18.4",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "clear-module": "^3.1.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^24.3.1",
    "jest-runner-eslint": "^0.7.3",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "dependencies": {
    "googleapis": "^37.2.0",
    "reflect-metadata": "^0.1.13"
  }
}
